{
  "name": "tenstack-starter",
  "version": "2.0.0",
  "description": "Typescript Node Express Rest API server starter",
  "private": true,
  "authors": "filoscoder",
  "license": "MIT",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,js}'",
    "transpile": "tsc",
    "clean": "rimraf dist",
    "build": "NODE_ENV=production run-s prettify clean transpile",
    "watch": "tsc --watch",
    "dev": "NODE_ENV=development nodemon",
    "service:start": "pm2 start ecosystem.config.js",
    "service:reload": "pm2 reload ecosystem.config.js",
    "service:startup": "pm2 startup",
    "service:stop": "pm2 stop ecosystem.config.js",
    "service:list": "pm2 list ecosystem.config.js",
    "service:delete": "pm2 delete ecosystem.config.js",
    "service:logs": "pm2 logs"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "rest",
    "api",
    "server"
  ],
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.2.0",
    "tsc": "^2.0.3",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
